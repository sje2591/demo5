<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo5.mapper.MemberMapper">
    <!-- 회원 목록 조회 -->
    <select id="selectMemberList" resultType="com.example.demo5.dto.MemberDTO">
        SELECT * FROM MEMBER
    </select>
    
    <!-- 회원 상세 조회 -->
    <select id="selectMemberDetail" resultType="com.example.demo5.dto.MemberDTO">
        SELECT * FROM MEMBER WHERE ID = #{ID}
    </select>
    
    <!-- 회원 삭제 -->
    <delete id="deleteMember" >
        DELETE FROM MEMBER WHERE ID = #{ID}
    </delete>
    
    </mapper>